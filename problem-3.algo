 PROCEDURE insertionSort(arr: array of integers)
    key:INTEGER := length(arr)
    i,j,n:INTEGER
    for i from 0 to n-1 do
        key := arr[i]
        j := i - 1
        while (j >= 0) and (arr[j] > key) do
            arr[j + 1] := arr[j]
            j := j - 1
        end while
        arr[j + 1] :=key
    end for
    end PROCEDURE